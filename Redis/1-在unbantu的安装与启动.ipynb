   "source": [
    "### 准备工作\n",
    "\n",
    "#### 安装：\n",
    "    sudo apt-get install redis-server\n",
    "    \n",
    "#### 卸载\n",
    "    sudo apt-get purge --auto-remove redis-server\n",
    "    \n",
    "#### 启动\n",
    "    redis安装后会默认启动，手动启动方式为：\n",
    "    sudo service redis-server start\n",
    "    \n",
    "#### 停止\n",
    "    sudo service redis-server stop\n",
    "    \n",
    "#### 查看\n",
    "    ps aux|grep redis\n",
    " "
   ]
  }
