    "### 执行命令的前提\n",
    "    \n",
    "    进入nginx的目录：\n",
    "    /usr/local/nginx/sbin\n",
    "    \n",
    "\n",
    "#### 查看版本\n",
    "\n",
    "    ./nginx -v\n",
    "    \n",
    "    \n",
    "#### 启动\n",
    "\n",
    "    ./nginx\n",
    "    \n",
    "#### 关闭\n",
    "\n",
    "    ./nginx -s stop\n",
    "    \n",
    "    \n",
    "#### 重新加载配置文件\n",
    "\n",
    "    ./nginx -s reload\n",
    "    "
   ]
